{
   "name": "image-repository",
   "version": "1.0.0",
   "description": "shopify backend internship challenge",
   "main": "dist/server.js",
   "scripts": {
      "start": "NODE_ENV=production node dist/server.js",
      "start:background": "NODE_ENV=production pm2 start dist/server.js --name 'chillspot-server'",
      "start:dev": "nodemon",
      "start:prod": "npm run build && npm run start:background",
      "build": "rimraf ./dist && tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
      "test": "cross-env NODE_ENV=test jest --detectOpenHandles"
   },
   "homepage": "https://github.com/Mayowa-Ojo/chillspot-server/#README",
   "repository": "github:Mayowa-Ojo/chillspot-server",
   "keywords": [
      "typescript",
      "jwt",
      "nodejs"
   ],
   "engines": {
      "node": "14.x.x",
      "npm": "6.x.x"
   },
   "author": "Mayowa Ojo",
   "license": "ISC",
   "dependencies": {
      "bcrypt": "^5.0.0",
      "class-validator": "^0.12.2",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "helmet": "^4.3.1",
      "http-status-codes": "^2.1.4",
      "jsonwebtoken": "^8.5.1",
      "rimraf": "^3.0.2",
      "winston": "^3.3.3"
   },
   "devDependencies": {
      "@types/bcrypt": "^3.0.0",
      "@types/cors": "^2.8.9",
      "@types/express": "^4.17.9",
      "@types/jest": "^26.0.19",
      "@types/jsonwebtoken": "^8.5.0",
      "@types/node": "^14.14.9",
      "@types/supertest": "^2.0.10",
      "@types/validator": "^13.1.0",
      "@types/winston": "^2.4.4",
      "cross-env": "^7.0.3",
      "jest": "^26.6.3",
      "nodemon": "^2.0.6",
      "supertest": "^6.0.1",
      "ts-jest": "^26.4.4",
      "ts-node": "^9.0.0",
      "tsconfig-paths": "^3.9.0",
      "tscpaths": "0.0.9",
      "tslint": "^6.1.3",
      "typescript": "^4.1.2"
   }
}
